    <!-- Required meta tags -->
    <meta charset="utf-8">
    <title>Renova Food - Sistema de Delivery</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="Renova Food - Sistema de Delivery" content="yes">
    <meta name="HandheldFriendly" content="True">
    <link rel="icon" href="favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="/templateMob/lib/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/templateMob/css/normalize.css">
