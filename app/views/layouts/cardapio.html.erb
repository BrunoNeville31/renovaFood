            <div class="logo"><%= $empresaMenu.nome %></div>
            <button type="button" id="sidebarrightbutton" class="btn">
              <i class="fas fa-shopping-basket"></i>
            </button>
          </div>
        </nav>
        <%= yield%>
        
        <div class="footer" style="background-color: #9a0e0e">
          <div class="footer-heading">Cad√°pio Admnistrado por</div>
          <div class="socmed">
